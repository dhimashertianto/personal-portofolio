{
  "extends": "next/core-web-vitals",
  "rules": {
    "no-unused-vars": "off",
    "quotes": ["error", "single"],
    "semi": "off",
    "no-duplicate-imports": [
      "error",
      {
        "includeExports": true
      }
    ],
    "no-return-assign": "error",
    "no-restricted-modules": [
      "error",
      {
        "paths": ["foo-module", "bar-module"],
        "patterns": ["foo-module/private/*", "bar-module/*", "!baz-module/good"]
      }
    ],
    "no-console": "error"
  }
}
